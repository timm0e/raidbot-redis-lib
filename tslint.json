{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "trailing-comma": [
            true,
            {
                "multiline":
                {
                    "functions": "never"
                }
            }
        ]
    },
    "rulesDirectory": []

}